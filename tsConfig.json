{
  "compilerOptions": {
    "target": "es2023",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "sourceMap": true

  },
  "include": ["**/*.ts", "src"]
}